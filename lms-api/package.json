{
  "name": "lms-revamp",
  "version": "1.0.0",
  "description": "LMS Node.js 22 Boilerplate Setup Guide Date Created: July 4, 2025, 12:46 PM PKTAuthors: Jidat IT Development Team Purpose: This guide provides a step-by-step process to set up a reusable Node.js 22 boilerplate with TypeScript, Sequelize for MySQL, Winston logging, Helmet and XSS security, Swagger documentation, ESLint, and Prettier.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "NODE_ENV=test jest",
    "migrate": "sequelize-cli db:migrate --env development --config src/config/config.json --migrations-path migrations",
    "migrate:undo": "sequelize-cli db:migrate:undo --env development --config src/config/config.json --migrations-path migrations",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all --env development --config src/config/config.json --migrations-path migrations",
    "seed": "sequelize-cli db:seed:all --env development --config src/config/config.json --seeders-path seeders",
    "seed:undo": "sequelize-cli db:seed:undo:all --env development --config src/config/config.json --seeders-path seeders",
    "migrate:prod": "sequelize-cli db:migrate --env production --config src/config/config.json --migrations-path migrations",
    "migrate:undo:prod": "sequelize-cli db:migrate:undo --env production --config src/config/config.json --migrations-path migrations",
    "migrate:undo:all:prod": "sequelize-cli db:migrate:undo:all --env production --config src/config/config.json --migrations-path migrations",
    "seed:prod": "sequelize-cli db:seed:all --env production --config src/config/config.json --seeders-path seeders",
    "seed:undo:prod": "sequelize-cli db:seed:undo:all --env production --config src/config/config.json --seeders-path seeders",
    "test-db": "ts-node src/utils/dbTest.ts" 
  },
